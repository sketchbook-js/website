{
  "name": "website",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "prettier -c {components,pages}/**/*.js",
    "sketchbook:start": "webpack --config sketchbook/webpack.config.js --mode development && sketchbook start sketchbook/example.json -p 3001",
    "sketchbook:build": "webpack --config sketchbook/webpack.config.js --mode production && sketchbook build sketchbook/example.json",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "next": "^10.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "styled-jsx/babel"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@sketchbook-js/sketchbook": "^0.3.0-4",
    "@storybook/react": "^6.3.4",
    "babel-loader": "^8.2.2",
    "prettier": "^2.2.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
